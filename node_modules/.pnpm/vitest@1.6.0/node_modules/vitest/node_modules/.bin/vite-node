#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxbochman/Desktop/code/240610-protocol/node_modules/.pnpm/vite-node@1.6.0/node_modules/vite-node/node_modules:/Users/maxbochman/Desktop/code/240610-protocol/node_modules/.pnpm/vite-node@1.6.0/node_modules:/Users/maxbochman/Desktop/code/240610-protocol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxbochman/Desktop/code/240610-protocol/node_modules/.pnpm/vite-node@1.6.0/node_modules/vite-node/node_modules:/Users/maxbochman/Desktop/code/240610-protocol/node_modules/.pnpm/vite-node@1.6.0/node_modules:/Users/maxbochman/Desktop/code/240610-protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-node@1.6.0/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../vite-node@1.6.0/node_modules/vite-node/vite-node.mjs" "$@"
fi
